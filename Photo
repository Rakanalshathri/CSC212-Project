public class Photo {
    private String path;
    private MyLinkedList<String> tags;

    public Photo(String path, MyLinkedList<String> tags) {
        this.path = path;
        this.tags = tags;
    }

    // Returns the photoâ€™s file path
    public String getPath() {
        return path;
    }

    // Returns the list of tags associated with the photo
    public MyLinkedList<String> getTags() {
        return tags;
    }
}
